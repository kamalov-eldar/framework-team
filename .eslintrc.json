{
  "extends":
  ["airbnb", "plugin:react-hooks/recommended"],
  "rules": {
    "jsx-a11y/no-static-element-interactions": "off",
    "jsx-a11y/click-events-have-key-events": "off",
    "no-console": "off",
    "linebreak-style": 0
  },
  "env": {
    "es6": true,//
    "browser": true,//
    "node": true//
  },
  "parserOptions":    {
    "ecmaVersion": 2020
  }
}

